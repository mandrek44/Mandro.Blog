<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Mandro.Blog" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2014-01.2.3">
  <WorkerRole name="Mandro.Blog.Worker" vmsize="ExtraSmall">
    <Runtime executionContext="elevated" />
    <Imports>
      <Import moduleName="Diagnostics" />
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
    </Imports>
    <Endpoints>
      <InputEndpoint name="Main" protocol="tcp" port="80" localPort="80" />
    </Endpoints>
    <ConfigurationSettings>
      <Setting name="BlogStorage" />
    </ConfigurationSettings>
  </WorkerRole>
</ServiceDefinition>